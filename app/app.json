{
  "expo": {
    "name": "Attivati",
    "slug": "attivati",
    "version": "1.1.5",
    "orientation": "portrait",
    "icon": "./src/assets/images/icon.png",
    "scheme": "attivati",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./src/assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "jsEngine": "hermes",
    "ios": {
      "bundleIdentifier": "it.volontariatotrentino.attivati",
      "supportsTablet": false,
      "buildNumber": "13"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/adaptive-icon-fg.png",
        "backgroundColor": "#ffffff"
      },
      "package": "it.volontariatotrentino.attivati",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBHAIlIrdW_sepiJyVuBE8KDaosCNwvQK0"
        }
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-localization",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Consenti a $(PRODUCT_NAME) di accedere alla tua posizione per poterti segnalare le opportunità di volontariato più vicine a te."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Consenti a $(PRODUCT_NAME) di accedere alle tue foto per selezionare una foto per il tuo profilo di volontario.",
          "cameraPermission": "Consenti a $(PRODUCT_NAME) di accedere alla tua fotocamera per selezionare una foto per il tuo profilo di volontario."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "21dc5681-9130-4e96-8575-8563818a3c32"
      }
    },
    "owner": "metadonors"
  }
}
